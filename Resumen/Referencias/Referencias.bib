  file     = {:C\:/Users/david/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Yan et al. - 2015 - Exact solutions of bending deflections for nano-beams and nano-plates based on nonlocal elasticity theory.pdf:pdf},
  issn     = {02638223},
  journal  = {Composite Structures},
  keywords = {Bending deflection,Nano-beam,Nano-plate,Nonlocal elasticity theory,Scale effect},
