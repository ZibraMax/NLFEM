  issn     = {00207225},
  journal  = {International Journal of Engineering Science},
  keywords = {Bending,Eringen integral model,Nanobeams,Nonlocal,Paradox},
  pages    = {107--116},
  title    = {{Bending of Euler-Bernoulli beams using Eringen's integral formulation: A paradox resolved}},
