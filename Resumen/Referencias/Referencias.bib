  journal  = {Physics Letters, Section A: General, Atomic and Solid State Physics},
  number   = {3},
  pages    = {236--242},
  title    = {{Application of nonlocal continuum mechanics to static analysis of micro- and nano-structures}},
  volume   = {363},
