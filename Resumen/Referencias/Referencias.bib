  keywords  = {Bending stiffness,Nano-cantilever beam,Nonlocal theory,Plane stress model,Scale effect factor},
  pages     = {47--57},
  publisher = {Elsevier Ltd},
  title     = {{Comments on nonlocal effects in nano-cantilever beams}},
  url       = {http://dx.doi.org/10.1016/j.ijengsci.2014.11.006},
  volume    = {87},
  year      = {2015}
}
